{  
  "manifest_version": 2,

  "name": "Tabber",
  "description": "You can manage your tabs easily using tabber",
  "icons": {
	"16": "Tabber16.png",
	"48": "Tabber48.png",
	"128": "Tabber128.png"
	   },
  "commands" : {
"_execute_browser_action": {
        "suggested_key": {
          "windows": "Alt+T",
          "mac": "Alt+T",
          "linux": "Alt+T"
        }
      }
    },
  "version": "1.0",
  "permissions": [
          "tabs"
        ],
  "browser_action": {
    "default_title": "Click here to manage tabs",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
